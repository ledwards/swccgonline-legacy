@charset "utf-8";
/* CSS Document */

body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, code, form, fieldset, legend, input, textarea, p, blockquote, th, td { margin:0; padding:0; }
div.left, div.right { min-height: 50px; background: #434343 url(/images/box_bottom.gif) left bottom no-repeat; position: relative; font: 11px 'Lucida Grande','Lucida Sans Unicode',Arial,Verdana,sans-serif; color: #898989;}
div.left { float: left; width: 283px; margin-right: 50px; margin-left: 20px; padding-top: 25px; margin-bottom: 35px; }
div.right { float: right; width: 283px; margin-left: 50px; padding-top: 25px; margin-bottom: 35px; }
div.center { width: auto; height: 55px; text-align: center; position: relative; margin: 0 298px 0 298px; padding: 55px 0 0 0; background: url(/images/user_page.gif) top center no-repeat;}
div.left h1, div.right h1{ position: absolute; top: 0; left: 0; background: url(/images/box_header.gif) top left no-repeat; height: 21px; font: 9px/20px 'Lucida Grande','Lucida Sans Unicode',Verdana, sans-serif; letter-spacing: 1px; font-weight: bold; color: #f0f0f0; display: block; text-transform: uppercase; width: 100%; text-indent: 6px; }
div.center h1{ display: none }

#nav ul li a:hover, #nav ul li#community a, #nav ul li#community a:hover { background: url(/images/whiteoverlay.png) top center repeat-x; color: #fff;}

div.center ul li { font: 9px/20px 'Lucida Grande','Lucida Sans Unicode',Verdana, sans-serif; color: #f0f0f0; }
div.center p { font: 11px 'Lucida Grande','Lucida Sans Unicode',Arial,Verdana,sans-serif; color: #ffffff; }
.post_section { margin-top: 25px; text-align: left; }

input.button { border: none; background: url(/images/button.png) 0px 0px no-repeat; font-family: "Lucida Grande","Lucida Sans Unicode",Arial,Verdana,sans-serif;
	font-size: 12px; color: #fad575;  padding-top: 5px; text-align: center; height:30px; width: 84px; padding-bottom: 8px;clear: both; margin-bottom: 10px;}
input.button:hover { background-position: 0px -29px; color: #fff; cursor: pointer; }
input.button:active { background-position: 0px -58px; color: #fff; cursor: pointer; }

#settings_center { margin: 55px auto 0 auto; display: block; z-index: 5; width: 300px; text-align: center; }
div#settings_center { margin: 0 auto; width:800px; background: url(/images/settings.gif) top center no-repeat; margin-top: -25px; }
div#settings_center h2 { margin-bottom: 20px; margin-top: 50px; text-align: left; }
div#settings_center input.field { width: 222px; height: 27px; background: url(/images/login_box_bg.png) top left no-repeat; text-align: center; font: 10px "Lucida Grande", "Lucida Sans Unicode", Verdana; color: #CCCCCC; letter-spacing: 1px; overflow: hidden; padding: 6px 0; display:block; border: none;}

div.settings { float: left; margin-top: 0px; width: 500px; text-align: left; }
div.settings_right { margin-top: 120px; margin-bottom: 50px; float: right; margin-left: 0; width: 222px; text-align: center; }
div.settings_left { float: left; text-align: right; margin-right: 0; width: 222px; background: #1b1b1b; padding: 5px; margin-top: 20px; }
div.settings_left p { font: 12px 'Lucida Grande','Lucida Sans Unicode',Arial,Verdana,sans-serif; color: #f0f0f0; }
div.button_container { margin: 0 auto; text-align: right; margin-top: 10px;}

div.errors { background: #ff2c2c; float: right; width: 283; margin-left: 50px; float: right;
			 font: 11px 'Lucida Grande','Lucida Sans Unicode',Arial,Verdana,sans-serif; color: #ffffff; }

ul#left_nav { margin-bottom: 10px; }
ul#left_nav li a { font-size: 12px; }

div.settings_left div label { display: block; padding-top: 12px; padding-bottom: 12px; font: 12px 'Lucida Grande','Lucida Sans Unicode',Arial,Verdana,sans-serif; color: #f0f0f0; }
div.settings_right img { margin: 10px; }
div.photo_input { border: 1px solid gray; margin: 10px; }
#user_photo { margin-bottom: 15px; margin-top: 15px; }
#user_about_me { margin: 5px; margin-top: 10px; margin-bottom: 10px; background-color: #434343; color: #ffffff; font: 11px "Lucida Grande","Lucida Sans Unicode",Arial,Verdana,sans-serif; }

div.left { z-index: 50; }

div.remember_me { margin-bottom: 10px; padding-top: 5px; text-align: left;}
div.center div.remember_me label { margin-left: 20px; display: inline; font: 12px 'Lucida Grande','Lucida Sans Unicode',Arial,Verdana,sans-serif; color: #f0f0f0; }
div.center div.remember_me #user_remember_me { display: inline; }
div.checkbox { position: absolute; display: inline; margin-top: -1px; margin-right: 5px; }

div#photo_description { margin-top: 7px; }
div#about_me_description { margin-top: 121px ;}
div#email_description { margin-top: -5px; }
div#profile_description { margin-top: -5px; }
div#remember_me_description { margin-top: -7px; }

div#password_description { padding-top: 0px; }
div#photo_description { padding-top: -2px; }
div#about_me_description { padding-top: 30px; }

div#password_form { margin-top: 47px; }